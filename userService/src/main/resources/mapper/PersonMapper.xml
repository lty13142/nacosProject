<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lty.user.sys.mapper.PersonMapper">

    <select id="getPersonById" parameterType="string" resultType="com.lty.user.sys.vo.PersonVo">
        select *
        from
            t_person
        where
            id = #{id}
    </select>

</mapper>
